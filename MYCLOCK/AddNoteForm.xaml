<Window x:Class="MYCLOCK.AddNoteForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MYCLOCK"
        mc:Ignorable="d"
        Title="AddNoteForm" Height="600" Width="450" Background="#FF121212" Foreground="White">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition ></RowDefinition>
            <RowDefinition Height="5*" ></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <ListBox ItemsSource="{Binding Notes}" Grid.Row="2" BorderBrush="{x:Null}" Background="#FF989898" Foreground="White">
            <!--<ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel>
                        <Label></Label>
                        <TextBlock Text="{Binding MessageINFO}"></TextBlock>
                    </StackPanel>
                </DataTemplate>
            </ListBox.ItemTemplate>-->
        </ListBox>
    </Grid>
</Window>
